{
  "name": "@react-typescript/config",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "description": "Node runnable TypeScript config for the monorepo",
  "main": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm --out-dir dist --dts",
    "clean": "rimraf dist",
    "type-check": "tsc --noEmit",
    "lint": "pnpm exec eslint -c ./eslint.config.js \"src/**/*.{ts,tsx,js}\"",
    "lint:fix": "pnpm exec eslint -c ./eslint.config.js \"src/**/*.{ts,tsx,js}\" --fix"
  },
  "devDependencies": {
    "@types/node": "22.13.10",
    "rimraf": "6.0.1",
    "tsup": "8.3.5",
    "tslib": "2.8.1",
    "tsx": "4.19.2",
    "typescript": "5.9.2"
  }
}
