{
  "name": "web-worker",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:offline": "jest offline-routing.test.ts",
    "test:online": "jest online-routing.test.ts",
    "test:both": "jest offline-routing.test.ts online-routing.test.ts",
    "test:offline:watch": "jest offline-routing.test.ts --watch",
    "test:online:watch": "jest online-routing.test.ts --watch",
    "test:both:watch": "jest offline-routing.test.ts online-routing.test.ts --watch",
    "test:report": "tsx scripts/generate-offline-test-report.ts",
    "test:report:offline": "tsx scripts/generate-offline-test-report.ts",
    "test:report:online": "tsx scripts/generate-online-test-report.ts",
    "test:report:both": "npm run test:report:offline && npm run test:report:online",
    "dev": "tsx scripts/test.ts"
  },
  "devDependencies": {
    "@types/jest": "29.5.12",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "ts-jest": "29.1.5",
    "tsx": "4.19.2",
    "typescript": "5.9.2"
  }
}
