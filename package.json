{
  "name": "react-typescript-monorepo",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "pnpm --filter web dev",
    "build": "pnpm --filter web build",
    "clean": "pnpm -r exec rm -rf node_modules dist build lib coverage",
    "clean:cache": "pnpm store prune",
    "install:clean": "pnpm clean && pnpm install",
    "preinstall": "npx only-allow pnpm"
  },
  "devDependencies": {
    "@eslint/js": "9.32.0",
    "@types/node": "22.13.10",
    "eslint": "9.32.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "globals": "16.3.0",
    "only-allow": "1.2.1",
    "prettier": "3.6.2",
    "rimraf": "6.0.1",
    "tsup": "8.3.5",
    "tsx": "4.19.2",
    "typescript": "5.9.2",
    "typescript-eslint": "8.39.0"
  },
  "packageManager": "pnpm@9.15.0",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=9.0.0"
  }
}
